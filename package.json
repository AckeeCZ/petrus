{
    "name": "ackee-redux-token-auth",
    "version": "1.0.0",
    "description": "Redux and redux saga logic for token authentication with an auto token refresh.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "build:lib": "cross-env BABEL_ENV=lib babel src --out-dir lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
        "build:dist": "webpack --config webpack.config.js",
        "build": "npm run build:dist && npm run build:es && npm run build:lib",
        "prepublish": "npm run build"
    },
    "author": "Jiri Smolik <smolikjirka@gmail.com>",
    "contributors": [
        "Marek Janča <marek.janca@ackee.cz>",
        "Lukáš Horák <lukas.horak@ackee.cz>"
    ],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://gitlab.ack.ee/Web/token-auth.git"
    },
    "dependencies": {
        "redux-saga": "^0.15.6"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "cross-env": "^5.1.3",
        "eslint": "^5.5.0",
        "eslint-config-ackee": "^0.9.0",
        "webpack": "^3.6.0"
    }
}
