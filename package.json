{
    "name": "ackee-redux-token-auth",
    "version": "1.1.0",
    "description": "Redux and redux saga logic for token authentication with an auto token refresh.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "build:lib": "cross-env BABEL_ENV=lib babel src --out-dir lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
        "build:dist": "webpack --config webpack.config.js",
        "build": "npm run build:dist && npm run build:es && npm run build:lib",
        "prepublish": "npm run build"
    },
    "author": "Jiri Smolik <smolikjirka@gmail.com>",
    "contributors": [
        "Marek Janča <marek.janca@ackee.cz>",
        "Lukáš Horák <lukas.horak@ackee.cz>"
    ],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://gitlab.ack.ee/Web/token-auth"
    },
    "dependencies": {
        "@babel/runtime": "^7.1.5",
        "localforage": "^1.7.3",
        "redux-saga": "^0.16.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.4",
        "cross-env": "^5.1.3",
        "eslint": "^5.5.0",
        "eslint-config-ackee": "^0.9.0",
        "eslint-config-prettier": "^3.3.0",
        "prettier": "^1.15.2",
        "prettier-config-ackee": "0.0.4",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2"
    }
}
