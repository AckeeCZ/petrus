{
    "extends": ["@ackee/eslint-config"],
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"],
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    },
    "overrides": [
        {
            "files": ["**/*.ts?(x)"],
            "rules": {
                "@typescript-eslint/no-unused-vars": "off",
                "no-unused-vars": "off"
            }
        }
    ]
}
